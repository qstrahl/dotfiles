[submodule "nvim"]
	path = nvim/.config/nvim
	url = git@github.com:qstrahl/.vim.git
	branch = master
	update = rebase
[submodule "tpm"]
	path = tmux/.tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm.git
	branch = master
	update = rebase
[submodule "urxvt-font-size"]
	path = urxvt/.urxvt/ext/urxvt-font-size
	url = https://github.com/majutsushi/urxvt-font-size.git
	branch = master
	update = rebase
[submodule "zsh-history-substring-search"]
	path = zsh/.config/zsh/zsh-history-substring-search
	url = https://github.com/zsh-users/zsh-history-substring-search.git
[submodule "tmuxifier"]
	path = tmux/.tmuxifier
	url = https://github.com/jimeh/tmuxifier.git
[submodule "zsh/.config/zsh/zsh-autopair"]
	path = zsh/.config/zsh/zsh-autopair
	url = git@github.com:hlissner/zsh-autopair.git
[submodule "zsh/.config/zsh/zsh-syntax-highlighting"]
	path = zsh/.config/zsh/zsh-syntax-highlighting
	url = https://github.com/zsh-users/zsh-syntax-highlighting.git
