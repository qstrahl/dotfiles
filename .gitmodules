[submodule "nvim"]
	path = nvim/.config/nvim
	url = git@github.com:qstrahl/.vim.git
	branch = master
	update = rebase
[submodule "tpm"]
	path = tmux/.tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm.git
	branch = master
	update = rebase
[submodule "urxvt-font-size"]
	path = urxvt/.urxvt/ext/urxvt-font-size
	url = https://github.com/majutsushi/urxvt-font-size.git
	branch = master
	update = rebase
[submodule "tmuxifier"]
	path = tmux/.tmuxifier
	url = https://github.com/jimeh/tmuxifier.git
[submodule "zsh/.config/zsh/antigen"]
	path = zsh/.config/zsh/antigen
	url = git@github.com:zsh-users/antigen.git
