[user]
	email = qstrahl@gmail.com
	name = Quinn Strahl
[push]
	default = simple
[branch]
	autosetuprebase = always
[github]
	user = qstrahl
[rebase]
	autoStash = true
[pull]
	rebase = true
[fetch]
	prune = true
[core]
	excludesfile = ~/.gitignore
[alias]
	amend = commit --amend --no-edit
	stat = status --short
[format]
	pretty = format:%C(auto)%h %s

[include]
	path = .gitconfig.local
